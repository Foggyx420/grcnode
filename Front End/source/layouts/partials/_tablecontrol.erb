<div class="row">
    <div class="col-xs-12" style="text-align:center;">
        <h3>Individual Project Rain (Wait until table loads!)</h3>
        <div class="col-xs-4">
            <input type="radio" name="RainType" id="RAC_Radio" value="RAC" style="margin-right:5px;" checked="checked"><label>RAC distribution</label><br/>
            <input type="radio" name="RainType" id="TRAC_Radio" value="TRAC" style="margin-right:5px;"><label>Historical RAC distribution</label><br/>
        </div>
        <div class="col-xs-4">
            <div class="row">
                <div class="col-xs-6">
                    <label>Quantity of coins:</label>
                </div>
                <div class="col-xs-6">
                    <input type="text" id="coins_input" name="Coins">
                </div>
            </div>
            <div class="row">
                <div class="col-xs-6">
                    <label>Account:</label>
                </div>
                <div class="col-xs-6">
                    <input type="text" id="rain_account" name="Account">
                </div>
            </div>
            <div class="row">
                <div class="col-xs-6">
                    <label>Message:</label>
                </div>
                <div class="col-xs-6">
                    <input type="text" id="rain_message" name="Message">
                </div>
            </div>
            <div class="row">
                <div class="col-xs-6">
                    <label>Required confirmations:</label>
                </div>
                <div class="col-xs-6">
                    <input type="text" id="rain_confirmations" name="Confirmations">
                </div>
            </div>
        </div>
        <div class="col-xs-4">
            <div class="row">
                <div class="col-xs-6">
                    <label>Run script: </label>
                </div>
                <div class="col-xs-6">
                    <form>
                        <button onclick="calculations.SendMany();return false;">Submit</button>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-6">
                    <label>Command output:</label>
                </div>
                <div class="col-xs-6">
                    <textarea id="SendMany"></textarea>
                </div>
            </div>
        </div>
    </div>
</div>